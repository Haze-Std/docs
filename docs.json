          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown-copied-1",
              "essentials/code-copied-1",
              "essentials/images-copied-1",
              "essentials/reusable-snippets-copied-1"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor-copied-1",
